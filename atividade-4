#include <stdio.h>

int main () {
	
	int num1;
	int num2;
	
	printf("Escolha um número\n");
	scanf("%i",&num1);
	printf("Escolha outro número\n");
	scanf("%i", &num2);
	
	if (num1 > num2) {
		printf("O primeiro número escolhido é o maior");
	}
	else {
		printf("O segundo número é maior");
	}
	
	return 0;
	
}
